<script type="text/javascript">*@
    $(document).ready(function () {
        validate();
        $('#CompanyName, #OwnerName,#CompanyAddress,#ZipCode,#Email,,#NoOfEmployee,#GSTNo,#Website').change(validate);
    });

    function validate() {
        if ($('#CompanyName').val().length > 0 &&
            $('#OwnerName').val().length > 0 &&
            $('#CompanyAddress').val().length > 0 &&
            $('#ZipCode').val().length > 0 &&
            $('#Email').val().length > 0 &&
            //$('#Email2').val().length > 0 &&
            //$('#CompanyPerfix').val().length > 0 &&
            //$('#HeaderFilePath').val().length > 0 &&
            //$('#FooterFilePath').val().length > 0 &&
            //$('#WhatsAppNo').val().length > 0 &&
            //$('#NoOfEmployee').val().length > 0 &&
            $('#GSTNo').val().length > 0 &&
            // $('#CompanyDescription').val().length > 0 &&
            $('#Website').val().length > 0 &&
                                                   // $('#CREG').val().length > 0
                                                ) {
            $("input[type=submit]").removeAttr('disabled');
        }
        else {
            $("input[type=submit]").attr('disabled', 'disabled');

        }
    }


    $("#Back").click(function () {
        location.replace("/SuperAdmin/GetCompanyList");

    });


    $("#CompanyRegistration").click(function () {
        var CompanyRegistration = $("#CompanyRegistration").val();






        var data = {

            CompanyRegistration: CompanyRegistration,


        }

        $.ajax({
            type: "POST",
            url: "/Admin/SaveCompanyRegistration",
            data: JSON.stringify(data),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (x) {
                alert("save success");
                window.location.reload(true);
            }
        });

        alert("save success");
        window.location.reload(true);


    });
</script>


