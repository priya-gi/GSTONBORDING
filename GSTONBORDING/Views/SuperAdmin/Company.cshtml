<script>
    $(document).ready(function () {

        $("#CountryId").change(function () {

            var id = $(this).val();
            $("#state").empty();

            $.get("../SuperAdmin/StateBind", { Countryid: id }, function (data) {
                var v = "<option>---Select---</option>";
                $.each(data, function (i, v1) {
                    v += "<option value=" + v1.Value + ">" + v1.Text + "</option>";
                });
                $("#state").html(v);
            });
        });

        $("#state").change(function () {
            var id = $(this).val();
            $("#CityId").empty();

            $.get("../SuperAdmin/CityBind", { stateid: id }, function (data) {
                var v = "<option>---Select---</option>";
                $.each(data, function (i, v1) {
                    v += "<option value=" + v1.Value + ">" + v1.Text + "</option>";
                });
                $("#CityId").html(v);
            });
        });
    });
